s\\ContactUs.js",[],"F:\\Web Development\\React\\Tutorials\\framer-portfolio\\src\\pages\\MovieDetails.js",["74"],"import React, { useState, useEffect } from \"react\";\r\nimport styled from \"styled-components\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { MovieDB } from \"../data/moviedb\";\r\nimport { motion } from \"framer-motion\";\r\nimport { pageAnimation } from \"../animation\";\r\n\r\nconst MovieDetails = () => {\r\n  const history = useHistory();\r\n  const url = history.location.pathname;\r\n  const [getMovies, setGetMovies] = useState(MovieDB);\r\n  const [movie, setMovie] = useState(null);\r\n\r\n  useEffect(() => {\r\n    const currentMovie = getMovies.filter((movie) => movie.url === url);\r\n    setMovie(currentMovie[0]);\r\n  }, [getMovies, url]);\r\n\r\n  return (\r\n    <>\r\n      {movie && (\r\n        <Details\r\n          variants={pageAnimation}\r\n          initial=\"hidden\"\r\n          animate=\"show\"\r\n          exit=\"exit\"\r\n        >\r\n          <HeadLine>\r\n            <h2>{movie.title}</h2>\r\n            <img src={movie.mainImg} alt=\"movie\" />\r\n          </HeadLine>\r\n          <Awards>\r\n            {movie.awards.map((award) => (\r\n              <Award\r\n                title={award.title}\r\n                description={award.description}\r\n                key={award.title}\r\n              />\r\n            ))}\r\n          </Awards>\r\n          <SecondImage>\r\n            <img src={movie.secondaryImg} alt=\"movie\" />\r\n          </SecondImage>\r\n        </Details>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nconst Details = styled(motion.div)`\r\n  color: #fff;\r\n  padding: 5rem;\r\n`;\r\nconst HeadLine = styled.div`\r\n  min-height: 90vh;\r\n  position: relative;\r\n\r\n  h2 {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n  }\r\n  img {\r\n    width: 100%;\r\n    height: 80vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n\r\n  h3 {\r\n    font-size: 1rem;\r\n  }\r\n\r\n  p {\r\n    margin-top: -2rem;\r\n  }\r\n\r\n  .line {\r\n    width: 50%;\r\n    background: #23d997;\r\n    height: 0.1rem;\r\n    margin: 1rem 0;\r\n  }\r\n`;\r\n\r\nconst SecondImage = styled.div`\r\n  img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  );\r\n};\r\n\r\nexport default MovieDetails;\r\n","F:\\Web Development\\React\\Tutorials\\framer-portfolio\\src\\data\\moviedb.js",[],"F:\\Web Development\\React\\Tutorials\\framer-portfolio\\src\\animation.js",[],"F:\\Web Development\\React\\Tutorials\\framer-portfolio\\src\\wave.js",[],"F:\\Web Development\\React\\Tutorials\\framer-portfolio\\src\\components\\Toggle.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":11,"column":21,"nodeType":"81","messageId":"82","endLine":11,"endColumn":33},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"no-unused-vars","'setGetMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]